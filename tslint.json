{
  "extends": ["tslint-config-airbnb", "tslint-config-prettier"],
  "rulesDirectory": ["tslint-plugin-prettier"],
  "rules": {
    "variable-name": false,
    "import-name": false,
    "semicolon": [true, "never"],
    "ter-arrow-parens": [true, "as-needed"],
    "trailing-comma": false,
    "prettier": [
      true,
      {
        "singleQuote": true,
        "semi": false
      }
    ]
  }
}
